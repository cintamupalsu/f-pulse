<%= form_with(model: @job_master, local: true) do |f|%>
    <%= render 'shared/error_messages' %>
    <%= f.label :content %>
    <%= f.text_field :content, class: 'form-control' %>

    <%= f.label :order %>
    <%= f.text_field :order, class: 'form-control' %>

    <%= f.submit "登録", class: "btn btn-primary" %>
<% end %>